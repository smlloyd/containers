{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "hostRules": [
    {
      "matchHost": "https://registry.redhat.io",
      "username": "{{ secrets.REDHAT_REGISTRY_USERNAME }}",
      "password": "{{ secrets.REDHAT_REGISTRY_PASSWORD }}"
    }
  ]
}
